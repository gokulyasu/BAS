<mvc:View controllerName="com.gokul.demo.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m" xmlns:f="sap.f">
	<App>
		<pages>
			<Page title="Hello">
				<content>
					<Button text="Plus" icon="sap-icon://add" press="onAdd"> </Button>
					<VBox>
						<f:GridList id="gridList" headerText="GridList header" items="{mMainModel>/FinalPage/Items}">
							<f:GridListItem>
								<VBox class="sapUiSmallMargin">
									<Input value="{mMainModel>value}"></Input>
									<layoutData>
										<FlexItemData growFactor="1" shrinkFactor="0"/>
										
									</layoutData>
								</VBox>
							</f:GridListItem>
						</f:GridList>
					</VBox>
					
					<HBox>
						<Button text="Bubble sort" press="onBubbleSort"></Button>
						<Button text="Alg 1"></Button>
						<Button text="Alg 2"></Button>
					</HBox>
					
				<Input value="{mMainModel>/FinalPage/output}" editable="false"></Input>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>